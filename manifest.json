{
    "manifest_version": 3,
    "name": "To-Do List",
    "version": "1.0",
    "description": "A simple to-do list extension",
    "permissions": ["storage"],
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'none';"
    }
}